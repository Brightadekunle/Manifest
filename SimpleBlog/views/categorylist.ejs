<a class="btn btn-primary" href="/blog/category/create">Create Category</a>
<br>
<br>
<% categories.forEach(function(category) { %>
    <li style="list-style: none;">
        <h4><a href="/blog/category/<%= category.id %>"><%= category.name %></a></h4>
         <a class="btn btn-primary" href="/blog/category/update/<%= category.id %>">Edit Category</a> | <a class="btn btn-danger" href="/blog/category/delete/<%= category.id %>">Delete Category</a>
    </li>
    <br/>
    <br>
<% }); %>